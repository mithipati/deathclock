  <div class="form-container">
    <img id="avatar1-form" src="/images/avatar1.png" />
  <form class="user-response" method="post" action="/results">
        <!-- Describes all open and multiple choice questions -->
        <div><label for="name">Name:<input type="text" id="name" placeholder="Your name goes here!" name="name"></label></div>

        <div><label for="age">Age:<input type="text" placeholder="Please enter your age" name="current_age"></label></div>

        <div class=""><label for="location">Location:<input type="text" placeholder="City, ST" name="location"></label></div>

        <div>
          <label for="gender">What's your Gender?:</label>
            <input type="radio" id="gender_male" name="gender" value="male"><label>Male</label>
            <input type="radio" id="gender_female" name="gender" value="female"><label>Female</label>
        </div>

        <div>
          <label for="diet">Diet:</label>
            <input type="radio" name="diet" value="vegetarian"><label>Vegetarian</label>
            <input type="radio" name="diet" value="vegan"><label>Vegan</label>
            <input type="radio" name="diet" value="meat"><label>Meat</label>
            <input type="radio" name="diet" value="children"><label>Children</label>
        </div>

        <div>
          <label for="outlook">What is your Outlook like?:</label>
            <input type="radio" name="outlook" value="cheerful"><label>Cheerful</label>
            <input type="radio" name="outlook" value="neutral"><label>Neutral</label>
            <input type="radio" name="outlook" value="negative"><label>Negative</label>
            <input type="radio" name="outlook" value="fuck"><label>Fuck Life</label>
        </div>

        <div>
          <label for="sleep">How often do you Sleep?:</label>
            <input type="radio" name="sleep" value="ten"><label>10+ Hours</label>
            <input type="radio" name="sleep" value="six"><label>6-10 Hours</label>
            <input type="radio" name="sleep" value="less-six"><label>Less than 6 Hours</label>
            <input type="radio" name="sleep" value="dead"><label>I'll sleep when I'm dead</label>
        </div>

      <!-- Describes all OR questions -->
      <div class="row">
        <div class="or-question-container">
          <div class="row or-question">
              <p>Left-Handed <span class="or-keyword">OR</span> Right-Handed?</p>
                <div class="large-6 columns left"><a href="#" class="button" id="left">Left-Handed</a></div>
                <div class="large-6 columns right"><a href="#" class="button" id="right">Right-Handed</a></div>
          </div>

          <div class="row or-question">
              <p>Sports <span class="or-keyword">OR</span> Video Games?</p>
                <div class="large-6 columns left"><a href="#" class="button" id="sports">Sports</a></div>
                <div class="large-6 columns right"><a href="#" class="button" id="video_games">Video Games</a></div>
          </div>

          <div class="row or-question">
              <p>Beer <span class="or-keyword">OR</span> Liquor</p>
                <div class="large-6 columns left"><a href="#" class="button" id="beer">Beer</a></div>
                <div class="large-6 columns right"><a href="#" class="button" id="liquor">Liquor</a></div>
          </div>

          <div class="row or-question">
              <p>Republican <span class="or-keyword">OR</span> Democrat?</p>
                <div class="large-6 columns left"><a href="#" class="button" id="republican">Republican</a></div>
                <div class="large-6 columns right"><a href="#" class="button" id="democrat">Democrat</a></div>
          </div>

          <div class="row or-question">
              <p>Dirty Sixth <span class="or-keyword">OR</span> Rainey?</p>
                <div class="large-6 columns left"><a href="#" class="button" id="dirty">Dirty Sixth</a></div>
                <div class="large-6 columns right"><a href="#" class="button" id="rainey">Rainey</a></div>
          </div>

          <div class="row or-question">
              <p>Sega <span class="or-keyword">OR</span> Nintendo?</p>
                <div class="large-6 columns left"><a href="#" class="button" id="sega">Sega</a></div>
                <div class="large-6 columns right"><a href="#" class="button" id="nintendo">Nintendo</a></div>
          </div>

          <div class="row or-question">
              <p>Ruby <span class="or-keyword">OR</span> JavaScript?</p>
                <div class="large-6 columns left"><a href="#" class="button" id="ruby">Ruby/JavaScript</a></div>
                <div class="large-6 columns right"><a href="#" class="button" id="css">CSS</a></div>
          </div>

       </div>
     </div>

     <!-- Describes the submit button -->
     <div class="submit-container">
        <input class="form-submit" type="submit" value="When do I meet my maker?">
     </div>

     <div class="invisible-container">
        <input type="text" id="question-handed" name="handed">
        <input type="text" id="question-sport-vid" name="sport_vid">
        <input type="text" id="question-beer_liq" name="beer_liq">
        <input type="text" id="question-rep_dem" name="rep_dem">
        <input type="text" id="question-dirty_rainey" name="dirty_rainey">
        <input type="text" id="question-sega_nin" name="sega_nin">
        <input type="text" id="question-ruby_js" name="ruby_js">
     </div>


  </form>
</div>



